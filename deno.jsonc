{
  "tasks": {
    "dev": "deno run --watch main.ts"
  },
  "fmt": {
    "singleQuote": true,
    "indentWidth": 2,
    "useTabs": false,
    "semiColons": true
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-explicit-any",
        "no-unused-vars",
        "prefer-const"
      ]
    }
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/fs": "jsr:@std/fs@^1.0.17"
  }
}
